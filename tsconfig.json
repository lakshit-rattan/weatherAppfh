{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "types": ["jest", "node"],
    "paths": {
      "@/*": ["./src/*"],
      "react-native": [
        "./node_modules/react-native",
        "../../node_modules/nativewind"
      ]
    },
    "allowImportingTsExtensions": false,
    "module": "esnext",
  },
  "include": [
    "./src/**/*",
    "index.d.ts",
    ".eslintrc.js",
    "jest.setup.js",
    "jest.config.js",
    "babel.config.js",
    "index.js"
  ],
  
}
